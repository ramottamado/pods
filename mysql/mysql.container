[Unit]
Description=MySQL Server

[Container]
Image=docker.io/library/mysql:9
AutoUpdate=registry
Network=saoirse.network
DNS=10.192.0.2
IP=10.192.0.99
IP6=fd00::99
HostName=mysql.saoirse.home.arpa
NetworkAlias=mysql.saoirse.home.arpa
Secret=mysql-password,type=env,target=MYSQL_ROOT_PASSWORD
Volume=mysql.volume:/var/lib/mysql:z
Memory=1g
